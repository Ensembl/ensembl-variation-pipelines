nextflow_process {

    name "Test Process createRankFile"
    script "nf_modules/create_rank_file.nf"
    process "createRankFile"

    test("Should run without failures") {

        when {
            process {
                """
                input[0] = "$baseDir/nf_config/variation_consequnce_rank_test.json"
                """
            }
        }

        then {
            assert process.success
            assert snapshot(process.out).match()
            assert process.trace.tasks().size() == 1
        }

    }

}
